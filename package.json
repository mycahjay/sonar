{
  "name": "sonar-project",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "sonar": "node sonar.js",
    "test": "jest test/ && node sonar.js"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "testResultsProcessor": "jest-sonar-reporter",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-reporter.xml",
    "indent": 4
  },
  "devDependencies": {
    "@types/config": "^0.0.38",
    "@types/hapi__hapi": "^20.0.8",
    "@types/jest": "^26.0.23",
    "@types/qs": "^6.9.6",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "jest-sonar-reporter": "^2.0.0",
    "sonarqube-scanner": "^2.8.0",
    "supertest": "^6.1.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "@hapi/hapi": "^20.1.3",
    "@types/hapi": "^18.0.5",
    "config": "^3.3.6",
    "extend-error": "^0.0.2",
    "hapi-pino": "^8.3.0",
    "inflection": "^1.13.1",
    "jest": "^27.0.4",
    "pm2": "^5.0.0",
    "prom-client": "^13.1.0",
    "qs": "^6.10.1"
  }
